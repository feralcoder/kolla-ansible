[Unit]
Description=Docker Registry Daemon
After=docker.service
Requires=docker.service


[Service]
Type=oneshot
User=root
RemainAfterExit=yes
ExecStartPre=-/root/docker-registry-stop.sh
ExecStart=/root/docker-registry-start.sh
ExecStop=/root/docker-registry-stop.sh
[Install]
WantedBy=multi-user.target
