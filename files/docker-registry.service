[Unit]
Description=Docker Registry Daemon
After=docker.service
Requires=docker.service


[Service]
Type=oneshot
User=root
RemainAfterExit=yes
ExecStartPre=-/usr/local/bin/docker-registry-stop.sh
ExecStart=/usr/local/bin/docker-registry-start.sh
ExecStop=/usr/local/bin/docker-registry-stop.sh
[Install]
WantedBy=multi-user.target
